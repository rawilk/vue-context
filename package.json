  "bugs": {
    "url": "https://github.com/rawilk/vue-context/issues"
  },
  "homepage": "https://github.com/rawilk/vue-context#readme",
  "dependencies": {
    "vue": "^2.5.17",
    "vue-clickaway": "^2.2.2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
